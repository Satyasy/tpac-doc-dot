APP_NAME=DocDot
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

APP_LOCALE=id
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=id_ID

APP_MAINTENANCE_DRIVER=file

# ============================================
# GEMINI AI - Wajib diisi untuk fitur chatbot
# ============================================
# Dapatkan di: https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_EMBEDDING_MODEL=gemini-embedding-001
GEMINI_LLM_MODEL=gemini-2.0-flash

# ============================================
# PINECONE - Wajib diisi untuk fitur RAG
# ============================================
# Dapatkan di: https://www.pinecone.io/
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_HOST=https://your-index.svc.aped-xxxx.pinecone.io
PINECONE_INDEX=docdot

# ============================================
# GOOGLE OAUTH - Opsional (login with Google)
# ============================================
# Setup di: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_client_id
GOOGLE_CLIENT_SECRET=your_client_secret
GOOGLE_CLIENT_REDIRECT=http://localhost:8000/auth/google/callback

BCRYPT_ROUNDS=12

# ============================================
# DATABASE - Sesuaikan dengan setup kamu
# ============================================
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=docdot
DB_USERNAME=root
DB_PASSWORD=

# ============================================
# LOGGING
# ============================================
LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# ============================================
# SESSION & CACHE
# ============================================
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database

# ============================================
# MAIL - Untuk OTP verification
# ============================================
# Pakai Mailtrap untuk testing: https://mailtrap.io/
# Atau SMTP Gmail, dll
MAIL_MAILER=smtp
MAIL_HOST=sandbox.smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=your_mailtrap_username
MAIL_PASSWORD=your_mailtrap_password
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS="noreply@docdot.com"
MAIL_FROM_NAME="DocDot"

VITE_APP_NAME="${APP_NAME}"
